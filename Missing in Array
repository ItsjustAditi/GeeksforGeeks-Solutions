class Solution {
    int missingNum(int arr[]) {
        // code here
        int i,sum1=0,sum=0,ans;
         int n=arr.length;
        for(i=0;i<n;i++)
        {
            sum1=sum1+arr[i];
        }
        for(i=1;i<=n+1;i++)
        {
            sum=sum+i;
        }
        ans=sum-sum1;
        return ans;
    }
}
